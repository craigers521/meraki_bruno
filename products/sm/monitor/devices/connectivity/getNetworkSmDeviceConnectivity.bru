meta {
  name: getNetworkSmDeviceConnectivity
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/networks/:networkId/sm/devices/:deviceId/connectivity
  body: none
  auth: bearer
}

params:query {
  ~perPage: {{perPage}}
  ~startingAfter: {{startingAfter}}
  ~endingBefore: {{endingBefore}}
}

params:path {
  networkId: {{networkId}}
  deviceId: {{deviceId}}
  basePath: {{basePath}}
}

headers {
  Accept: application/json
}

auth:bearer {
  token: {{apiKey}}
}

docs {
  Returns historical connectivity data (whether a device is regularly checking in to Dashboard).
}
